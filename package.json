{
  "name": "@ezcater/recipe",
  "version": "12.0.2",
  "description": "ezCater's React UI Component library",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ezcater/recipe"
  },
  "peerDependencies": {
    "react": "^16.8.2 || ^17.0.0",
    "react-dom": "^16.8.2 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.10.0",
    "@ezcater/ez-scripts": "^1.0.2",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/react": "^6.2.9",
    "@testing-library/dom": "^7.22.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "@types/cheerio": "^0.22.15",
    "@types/jest": "^24.0.25",
    "@types/jest-axe": "^3.2.1",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "clean-css": "^4.2.3",
    "concurrently": "^5.0.2",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "csstype": "^2.6.8",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-cli": "^24.9.0",
    "jest-environment-jsdom": "^26.3.0",
    "jest-junit": "^12.0.0",
    "jest-raw-loader": "^1.0.1",
    "jest-standard-reporter": "^2.0.0",
    "jest-transform-css": "^2.1.0",
    "lz-string": "^1.4.4",
    "parcel": "^2.0.0-nightly.435",
    "playroom": "^0.23.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "sosia": "^1.2.6",
    "sosia-markdown": "^1.1.2",
    "sosia-remote-puppeteer": "^1.1.3",
    "sosia-types": "^1.0.4",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "start": "concurrently \"npm run build:watch\" \"(cd doc-site/ && npm run develop)\" \"npm run playroom:start\" --kill-others",
    "start:with-ie11": "concurrently \"npm run build:watch\" \"(cd doc-site/ && npm run develop:with-ie11)\" --kill-others",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:dependencies && npm run build:types && npm run build:cjs && npm run build:esm && cp .npmignore dist",
    "build:types": "tsc --emitDeclarationOnly --rootDir src",
    "build:dependencies": "cd packages/@stitches/core && npm run build",
    "build:packages": "rm -rf .parcel-cache && parcel build packages/@recipe-ui/*/",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:watch": "npm run build:cjs && npm run build:esm -- --watch",
    "build:docs:prod": "cd doc-site && npm run build:prod",
    "install:dependencies": "cd packages/@stitches/core && npm install && npm run patch",
    "push:docs": "cd doc-site && npm run deploy",
    "deploy": "npm run build:docs:prod && npm run playroom:build && npm run push:docs",
    "bundle-size": "cd tree-shaking-size/ && npm i && npm run build && node check-size.js",
    "clean": "rm -rf dist",
    "create-component": "plop component",
    "changeset": "npx changeset",
    "release": "npm run build && npm changeset publish",
    "test": "cross-env NODE_OPTIONS=--max-http-header-size=512000 jest --coverage --no-cache",
    "test:watch": "npm test -- --watch",
    "test:ci": "cross-env NODE_OPTIONS=--max-http-header-size=512000 jest --ci --coverage --no-cache --maxWorkers=2 --reporters=jest-standard-reporter --reporters=jest-junit",
    "lint": "ez-scripts lint",
    "lint:fix": "ez-scripts lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build"
  },
  "dependencies": {
    "@changesets/cli": "^2.16.0",
    "@emotion/is-prop-valid": "^0.7.3",
    "@ezcater/snitches": "^0.0.11",
    "@popperjs/core": "^2.1.1",
    "@react-types/provider": "^3.1.1",
    "@recipe-ui/legacy-theme": "^1.0.0",
    "dayjs": "^1.8.14",
    "react-highlight-words": "^0.16.0"
  },
  "jest-junit": {
    "outputDirectory": "./reports/jest",
    "outputName": "junit.xml"
  }
}
