{
  "name": "@ezcater/recipe",
  "version": "11.4.2",
  "description": "ezCater's React UI Component library",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": [
    "lib/index.js",
    "lib/esm/index.js",
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ezcater/recipe"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-stage-1": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@ezcater/ez-scripts": "^1.0.2",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-essentials": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/react": "^6.1.6",
    "@testing-library/dom": "^7.22.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "@types/cheerio": "^0.22.15",
    "@types/jest": "^24.0.25",
    "@types/jest-axe": "^3.2.1",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "clean-css": "^4.2.3",
    "concurrently": "^5.0.2",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "csstype": "^2.6.8",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-cli": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "jest-environment-jsdom": "^26.3.0",
    "jest-junit": "^12.0.0",
    "jest-raw-loader": "^1.0.1",
    "jest-standard-reporter": "^2.0.0",
    "jest-transform-css": "^2.1.0",
    "parcel": "^2.0.0-nightly.435",
    "playroom": "^0.23.0",
    "plop": "^2.5.3",
    "postcss": "^8.1.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "sosia": "^1.2.6",
    "sosia-markdown": "^1.1.2",
    "sosia-remote-puppeteer": "^1.1.3",
    "sosia-types": "^1.0.4",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "concurrently \"npm run build:watch\" \"(cd doc-site/ && npm run develop)\" --kill-others",
    "start:with-playroom": "concurrently \"npm run playroom:start\" \"npm run start\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && cp .npmignore lib",
    "build:types": "tsc --emitDeclarationOnly --rootDir src",
    "build:packages": "rm -rf .parcel-cache && parcel build packages/@recipe-ui/*/",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir lib/esm --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:watch": "npm run build:cjs && npm run build:esm -- --watch",
    "build:docs:prod": "cd doc-site && npm run build:prod",
    "push:docs": "cd doc-site && npm run deploy",
    "deploy": "npm run build:docs:prod && npm run playroom:build && npm run push:docs",
    "bundle-size": "cd tree-shaking-size/ && npm i && npm run build && node check-size.js",
    "clean": "rm -rf lib",
    "create-component": "plop component",
    "test": "jest --coverage --no-cache",
    "test:watch": "npm test -- --watch",
    "test:ci": "jest --ci --coverage --no-cache --maxWorkers=2 --reporters=jest-standard-reporter --reporters=jest-junit",
    "lint": "ez-scripts lint",
    "lint:fix": "ez-scripts lint --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^0.7.3",
    "@ezcater/snitches": "0.0.2",
    "@popperjs/core": "^2.1.1",
    "@react-types/provider": "^3.1.1",
    "@recipe-ui/legacy-theme": "^1.0.0",
    "@stitches/core": "0.1.0-canary.12",
    "dayjs": "^1.8.14",
    "react-highlight-words": "^0.16.0",
    "styled-component-variant": "^1.0.1"
  },
  "jest-junit": {
    "outputDirectory": "./reports/jest",
    "outputName": "junit.xml"
  }
}
